{
  "name": "frontend-starterkit-react-v2",
  "version": "1.0.0",
  "description": "Front-end development starter kit for react redux version 2",
  "main": "index.js",
  "scripts": {
    "start": "gulp serve",
    "clean": "gulp clean",
    "dist": "gulp dist",
    "deploy": "gulp deploy",
    "test": "mocha ./app/src/js/specs -r mock-local-storage --compilers js:babel-core/register --recursive --require ./app/src/js/specs/setup.js --require ignore-styles",
    "test:watch": "npm test -- --watch",
    "test:watch-file": "mocha --compilers js:babel-core/register --recursive --require ./app/src/js/specs/setup.js --require ignore-styles --watch",
    "package": "npm install && npm run deploy && sbt package",
    "server": "node server.js",
    "eslint": "eslint app/src/js"
  },
  "repository": {
    "type": "git",
